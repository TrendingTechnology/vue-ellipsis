{
  "name": "@hyjiacan/vue-ellipsis",
  "description": "Customize ellipsis-like component for Vue2.",
  "version": "0.1.3",
  "main": "lib/ellipsis.common.js",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "node version.js & vue-cli-service build --target lib --dest=lib --formats=commonjs --name ellipsis src/components/index.js",
    "build:docs": "vue-cli-service build --dest=docs",
    "release": "yarn run lint & yarn run build & yarn run build:docs",
    "lint": "npx vue-cli-service lint"
  },
  "files": [
    "lib",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hyjiacan/vue-ellipsis.git"
  },
  "bugs": {
    "url": "https://github.com/hyjiacan/vue-ellipsis/issues"
  },
  "homepage": "https://hyjiacan.github.io/vue-ellipsis/",
  "keywords": [
    "vue",
    "slide"
  ],
  "author": "hyjiacan",
  "license": "MIT",
  "dependencies": {
    "core-js": "^2.6.5",
    "vue": "^2.6.10"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.8.0",
    "@vue/cli-plugin-eslint": "^3.8.0",
    "@vue/cli-service": "^3.8.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "vue-template-compiler": "^2.6.10"
  }
}
